<%-include('./partials/admin_header')-%>
<link rel="stylesheet" href="/css_files/admin_tableview_style.css">
<%-include('./partials/admin_sidebar')-%>


<div class="content_nav_container">
    <h1>Archived Inquiries</h1>
   <a href="/inquiries"><button class="yellow_button_design">Unresolved Inquiries</button></a>
</div>

<div class="main_content_container">
    <table>
        <tr>
            <th>Name of Inquirer</th>
            <th>Date Resolved</th>
            <th>Program</th>
            <th>Contact Number</th>
            <th>Child Name</th>
            <th> </th>
        </tr>
            <%inquiryArchive.forEach(inquiries => {%>
        <tr>
            <td><%= inquiries.name %></th>
            <td><%= inquiries.inquiryDate.toDateString().slice(4,15) %></td>
            <td><%= inquiries.program %></td>
            <td><%= inquiries.phoneNumber %></td>
            <td><%= inquiries.childName %></td>
            <td><a href="/view_inquiry_archives?id=<%= inquiries.id %>"><button>Advanced View</button></a></td>
        </tr>
            <%})%>
    </table>
</div>

<%-include('./partials/admin_footer') -%>